From: Alexander Georgievskiy <galeksandrp@gmail.com>
Date: Sat, 24 Jul 2021 18:28:59 +0300
Subject: [PATCH 3/3] CI

---
 .github/workflows/blank.yml | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/.github/workflows/blank.yml b/.github/workflows/blank.yml
index dee100f..f1c0890 100644
--- a/.github/workflows/blank.yml
+++ b/.github/workflows/blank.yml
@@ -17,10 +17,10 @@ jobs:
       run: git fetch --no-tags --unshallow origin rootfs-stock-ci
     - name: git format-patch
       run: git format-patch -o ~/patches $(git merge-base origin/rootfs-stock-ci HEAD)
-    - name: git fetch template
-      run: git fetch --no-tags origin template
-    - name: git checkout template
-      run: git checkout template
+    - name: git fetch template-test
+      run: git fetch --no-tags origin template-test
+    - name: git checkout template-test
+      run: git checkout template-test
     - name: cp patches
       run: cp -r ~/patches .
     - name: git add
@@ -39,11 +39,11 @@ jobs:
     - name: apt install
       run: sudo apt install -y liblzo2-dev mtd-utils squashfs-tools fakeroot
     - name: pip upgrade
-      run: python -m pip install --upgrade pip
+      run: sudo python -m pip install --upgrade pip
     - name: pip install
-      run: pip install python-lzo ubi_reader
+      run: sudo pip install python-lzo ubi_reader
     - name: make
-      run: make
+      run: sudo make -j$(nproc)
 
     - uses: actions/upload-artifact@v2
       with:
